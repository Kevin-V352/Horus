dlerDelete = name => dispatch(deleteFavorite(name));\r\n\r\n    useEffect(() => {\r\n        axios.get(`https://api.openweathermap.org/data/2.5/weather?lat=${lat}&lon=${lon}&appid=${API_KEY}&lang=es&units=metric`)\r\n            .then(response => {\r\n                setCurrentWeather({\r\n                    temp: response.data.main.temp,\r\n                    backgroundId: response.data.weather[0].icon,\r\n                    description: response.data.weather[0].description\r\n                });\r\n            })\r\n    }, []);\r\n\r\n    return (\r\n        <Link to='/Dashboard' onClick={handlerChange} className={styles.link}>\r\n            <Card className={classes.root}>\r\n                <CardContent>\r\n                    <Grid container>\r\n                        <Grid item xs={1} />\r\n                        <Grid item xs={10}>\r\n                            <Typography className={classes.title} >\r\n                                {name ? name : <Skeleton />}\r\n                            </Typography>\r\n                            <Typography className={classes.temp}>\r\n                                {currentWeather.temp || currentWeather.temp === 0 ? `${Math.round(currentWeather.temp)} °C` : <Skeleton />}\r\n                            </Typography>\r\n                            <div className={styles.container}>\r\n                                {\r\n                                    currentWeather.backgroundId ?\r\n                                        <img\r\n                                            src={`./svg-weather-icons/${currentWeather.backgroundId}.svg`}\r\n                                            alt=\"weather-icon\"\r\n                                            className={styles.icon}\r\n                                        />\r\n                                        :\r\n                                        <Skeleton className={classes.skeletonIcon}/>\r\n                                }\r\n\r\n                            </div>\r\n                            <Typography className={classes.description}>\r\n                                {\r\n                                    currentWeather.description ? \r\n                                    ChangeDescription(currentWeather.description)\r\n                                    :\r\n                                    <Skeleton/>\r\n                                }\r\n                            </Typography>\r\n                        </Grid>\r\n                        <Grid item xs={1}>\r\n                            <IconButton\r\n                                onClick={() => handlerDelete(name)}\r\n                                className={classes.button}\r\n                            >\r\n                                <BackspaceIcon className={classes.icon} />\r\n                            </IconButton>\r\n                        </Grid>\r\n                    </Grid>\r\n                </CardContent>\r\n            </Card>\r\n        </Link>\r\n    );\r\n};\r\n\r\nexport default CardLocation;","C:\\Users\\KV\\Estudios\\Proyectos personales\\Horus\\src\\components\\body\\favorites-list\\favorites-list-material-styles.js",[],"C:\\Users\\KV\\Estudios\\Proyectos personales\\Horus\\src\\components\\body\\favorites-list\\card-location\\card-location-material-styles.js",[],"C:\\Users\\KV\\Estudios\\Proyectos personales\\Horus\\src\\components\\body\\historical-weather\\historical-weather.jsx",["144"],"C:\\Users\\KV\\Estudios\\Proyectos personales\\Horus\\src\\components\\body\\historical-weather\\historical-weather-chart\\historical-weather-chart.jsx",["145","146","147","148","149"],"C:\\Users\\KV\\Estudios\\Proyectos personales\\Horus\\src\\components\\body\\historical-weather\\historical-weather-chart\\historical-weather-chart-material-styles.js",[],"C:\\Users\\KV\\Estudios\\Proyectos personales\\Horus\\src\\components\\body\\historical-weather\\historical-weather-material-styles.js",[],"C:\\Users\\KV\\Estudios\